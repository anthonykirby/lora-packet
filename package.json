{
  "name": "lora-packet",
  "version": "0.8.23",
  "author": "Anthony Kirby",
  "description": "LoRa packet decoder/encoder",
  "keywords": [
    "lora",
    "packet",
    "parse",
    "decrypt",
    "verify",
    "encode",
    "decode"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthonykirby/lora-packet.git"
  },
  "bugs": {
    "url": "https://github.com/anthonykirby/lora-packet/issues"
  },
  "main": "./cjs/lib.js",
  "exports": {
    "require": "./cjs/lib.js",
    "import": "./esm/lib.js"
  },
  "scripts": {
    "build": "rm -rf ./cjs; npx tsc; echo '{\"type\": \"commonjs\"}' > cjs/package.json",
    "build-esm": "rm -rf ./esm; npx tsc --module es2022 --outDir esm/; echo '{\"type\": \"module\"}' > esm/package.json",
    "test": "npx jest",
    "prepublishOnly": "npx eslint .; npm run build; npm run build-esm",
    "prepare": "npm run build; npm run build-esm",
    "linter": "npx ."
  },
  "bin": {
    "lora-packet-decode": "./cjs/cli.js"
  },
  "dependencies": {
    "aes-cmac": "1.0.3",
    "crypto-js": "4.1.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/crypto-js": "4.1.2",
    "@types/node": "14.11.8",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "ansi-regex": ">=5.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.7.0",
    "minimist": "^1.2.8",
    "prettier": "^2.8.8",
    "set-value": "^4.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5"
  },
  "contributors": [
    {
      "name" : "Kalik1",
      "url" : "https://github.com/kalik1"
    }
  ]
}
